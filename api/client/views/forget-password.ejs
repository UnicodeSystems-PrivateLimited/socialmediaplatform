<script>
        $(function(){
                $('form').submit(function(e){
                    e.preventDefault();
                    var frmData=$('form').serialize();
                    console.log("Sending Data:"+JSON.stringify(frmData));
                    $.post('api/user/forgetPassword',frmData,function(data){
                    console.log("received  Data:"+JSON.stringify(data));                        
                if(data.status===1){
                        console.log("in if");
                            $('.dev-login-msg').removeClass('alert-danger').addClass('alert-success').text(data.msg);
                            setTimeout(function(){
                            window.location.reload();
                            }, 2000);
                        }else{
                        console.log("in else");
                                $('.dev-login-msg').removeClass('alert-success').addClass('alert-danger').text(data.msg);
                        }
                        console.log("Data:"+JSON.stringify(data));
                    },'json');
                });
        });
</script>
